<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <table>
        <tr>
            <th> id </th>
            <th> username</th>
            <th> email</th>
            <th>  password</th>
        </tr>
      
    
        <% for(user of users){ %>
            <tr> 
                <td> <%=user.id%> </td>
                <td> <%=user.username%></td>
                <td> <%=user.email%></td>
                <td>  <%=user.password%></td>
                <td>  <form method="GET" action="/user/<%=user.id%>/edit">
                    <button> Edit name</button>
                    
                </form>
            </td>
            <td>
                <form method="POST" action="/user/<%=user.id%>/delete?_method=DELETE">
                    <button > delete</button>
                </form>
            </td>
               
                
            </tr>
           
       <% } %> 
    </table>
    

</body>
</html>